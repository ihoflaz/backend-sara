{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/swagger.json",
            "dest": "/index.js"
        },
        {
            "src": "/api-docs/(.*)",
            "dest": "/api-docs/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
