{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/swagger-ui.css",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "index.js"
        },
        {
            "src": "/api-docs/swagger-ui-bundle.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "index.js"
        },
        {
            "src": "/api-docs/swagger-ui-standalone-preset.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "index.js"
        },
        {
            "src": "/api-docs/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/swagger.json",
            "headers": {
                "Content-Type": "application/json"
            },
            "dest": "index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
